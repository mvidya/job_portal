<h1>Hi,Welcome to Job Type Profile</h1>
<%= form_for @job_type do |f| %>
<table>
	
	<tr>
		<td> Type </td>
		<td><%= f.text_field :type_name %></td>
	</tr>
	<tr>
		<td> Description </td>
		<td><%= f.text_area :type_description %></td>
	</tr>
</table>
<%= f.submit "Submit" %>
<% end %>

<style>
	.error{
		color: #ff0000;
	}	
</style>